    "def square(n):\n",
    "  return n * n\n",
    "numbers = [4, 5, 2, 9]       # numbers to be squared \n",
    "print(\"initial list: \",numbers)\n",
    "result = map(square, numbers)   # map function defined \n",
    "print(\"Square of the list :\")\n",
    "print(list(result))"
   ]
